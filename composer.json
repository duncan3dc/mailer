{
    "name": "duncan3dc/mailer",
    "type": "library",
    "description": "A simple mailer abstraction with a clean interface",
    "keywords": ["email", "mailer", "siwftmailer", "smtp"],
    "homepage": "https://github.com/duncan3dc/mailer",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Craig Duncan",
        "email": "git@duncanc.co.uk",
        "homepage": "https://github.com/duncan3dc",
        "role": "Developer"
    }],
    "require": {
        "duncan3dc/blade": "^4.0",
        "symfony/mailer": "^5.4 || ^6.0 || 7.0",
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "duncan3dc/object-intruder": "^0.3 || ^1.0",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^8.5.29"
    },
    "autoload": {
        "psr-4": {"duncan3dc\\Mailer\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"duncan3dc\\MailerTests\\": "tests/"}
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
